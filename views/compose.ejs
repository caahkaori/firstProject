<%- include("partials/header") %>

<h1> Compose </h1>

<form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input type="text" class="form-control" name="titleBox">
    </div>
    <div class="form-group">
     <label for="post">Post</label>
     <textarea class="form-control" rows="5" id="comment" name="bodyPost"></textarea>
   </div>
<button class="btn btn-primary" name="publishButton" type="submit">Publish</button>
</form>


